# n8n Workflow Generator Environment Variables

# Claude API (Primary AI for workflow generation)
CLAUDE_API_KEY=your_claude_api_key_here

# Next.js Authentication
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# n8n Integration
N8N_OAUTH_CLIENT_ID=your_n8n_oauth_client_id
N8N_OAUTH_CLIENT_SECRET=your_n8n_oauth_client_secret

# Supabase (if using for user management/storage)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_ACCESS_TOKEN=your_supabase_access_token

# Optional: Additional AI Provider APIs (for enhanced features)
OPENAI_API_KEY=your_openai_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
XAI_API_KEY=your_xai_api_key_here
AZURE_OPENAI_API_KEY=your_azure_openai_key_here
OLLAMA_API_KEY=your_ollama_api_key_here

# Development
NODE_ENV=development 